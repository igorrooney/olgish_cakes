{
  "name": "olgish_cakes",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "icons:maskable": "tsx scripts/generate-maskable-icons.ts",
    "icons:all": "tsx scripts/generate-icons.ts",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "seed": "node scripts/seed-cakes.ts",
    "cleanup": "node scripts/cleanup-cakes.ts",
    "reset-db": "pnpm run cleanup && pnpm run seed",
    "update-cakes": "node scripts/update-cakes.ts",
    "studio": "sanity studio",
    "deploy": "vercel deploy --prod",
    "seed:testimonials": "node scripts/seed-testimonials.js",
    "check:testimonials": "node scripts/check-testimonials.js",
    "migrate:faqs": "ts-node --project scripts/tsconfig.json scripts/migrateFaqs.ts",
    "migrate:descriptions": "node scripts/migrate-descriptions.js",
    "clear-cache": "node scripts/clear-cache.js",
    "clear-cache-dev": "node scripts/clear-cache-dev.js",
    "setup-webhook": "node scripts/setup-webhook.js",
    "test-webhook": "node scripts/test-webhook.js",
    "analyze": "ANALYZE=true next build",
    "performance": "node scripts/performance-monitor.js",
    "perf:check": "node scripts/quick-performance-check.js",
    "lighthouse": "lighthouse https://olgishcakes.co.uk --output=json --output-path=./reports/lighthouse-report.json",
    "lighthouse:test": "node scripts/lighthouse-test.js",
    "bundle-analyzer": "cross-env ANALYZE=true next build",
    "seo:optimize": "node scripts/seo-optimization.js",
    "seo:check": "node scripts/seo-optimization.js --check-only",
    "seo:report": "node scripts/seo-optimization.js --generate-report",
    "optimize:render-blocking": "node scripts/optimize-render-blocking.js",
    "optimize:checklist": "node scripts/optimize-render-blocking.js --checklist",
    "accessibility:check": "node scripts/accessibility-check.js",
    "accessibility:audit": "pnpm run accessibility:check && pnpm run lighthouse",
    "add-cake-links": "node scripts/add-cake-by-post-links.js",
    "update-card-images": "node scripts/update-blog-card-images.js",
    "backup:sanity": "node scripts/backup-simple.js",
    "backup:full": "node scripts/backup-with-options.js --include-assets",
    "backup:assets": "node scripts/backup-with-options.js --assets-only",
    "backup:schedule": "node scripts/backup-scheduler-simple.js",
    "backup:upload": "node scripts/backup-storage.js",
    "backup:sync": "node scripts/backup-storage.js --sync",
    "backup:stats": "node scripts/backup-storage.js --stats",
    "backup:run": "node scripts/backup-scheduler-simple.js --run",
    "backup:daily": "node scripts/backup-scheduler-simple.js --run daily",
    "backup:weekly": "node scripts/backup-scheduler-simple.js --run weekly",
    "backup:monthly": "node scripts/backup-scheduler-simple.js --run monthly",
    "backup:status": "node scripts/backup-scheduler-simple.js --status",
    "backup:setup": "node scripts/quick-setup.js",
    "backup:manager": "node scripts/backup-manager.js",
    "seo:ranking": "node scripts/advanced-seo-ranking.js",
    "seo:linking": "node scripts/internal-linking-optimizer.js",
    "seo:full-analysis": "npm run seo:ranking && npm run seo:linking"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.8.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@azure/storage-blob": "^12.17.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@google-cloud/storage": "^7.7.0",
    "@mui/icons-material": "5.15.10",
    "@mui/material": "5.15.10",
    "@mui/x-date-pickers": "^7.28.3",
    "@portabletext/react": "^3.2.1",
    "@sanity/client": "^6.29.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/vision": "^3.88.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "critters": "^0.0.25",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "framer-motion": "^11.0.3",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.1.3",
    "next-sanity": "^7.1.4",
    "next-themes": "^0.2.1",
    "node-cron": "^3.0.3",
    "nuqs": "^1.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^4.2.0",
    "sanity": "^3.88.1",
    "styled-components": "^6.1.8",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.3",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lighthouse": "^11.6.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@mui/material": "5.15.10",
    "@mui/icons-material": "5.15.10",
    "eslint": "8.56.0"
  },
  "peerDependencies": {
    "@mui/icons-material": "5.15.10",
    "@mui/material": "5.15.10"
  }
}
